{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": ["error", "only-multiline"],
    "linebreak-style": ["error", "windows"],
    "import/named": "off",
    "import/extensions": "off",
    "no-restricted-syntax": "off",
    "no-console": [
      "error",
      {
        "allow": ["log"]
      }
    ]
  }
}
